{
  "name": "vite-app",
  "version": "0.6.2",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "clean": "rimraf --no-glob ./dist ./tsconfig.tsbuildinfo ./node_modules/.vite",
    "serve": "vite preview",
    "test": "yarn test-unit",
    "test-unit": "echo 'No test present yet'",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/vite-app.eslintcache",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@your-org/common-i18n": "workspace:^",
    "@your-org/core-lib": "workspace:^",
    "@your-org/ui-lib": "workspace:^",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@emotion/babel-plugin": "11.9.2",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@vitejs/plugin-react": "1.3.2",
    "@your-org/eslint-config-bases": "workspace:^",
    "eslint": "8.17.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "typescript": "4.7.3",
    "vite": "2.9.10",
    "vite-tsconfig-paths": "3.5.0"
  }
}
